/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@super-abp/ng.permission-management": [
        "dist/permission-management",
        "projects/permission-management/src/public-api.ts"
      ],
      "@super-abp/ng.permission-management/proxy": [
        "dist/permission-management/proxy",
        "projects/permission-management/proxy/src/public-api.ts"
      ],
      "@super-abp/ng.region-management": [
        "projects/region-management/src/public-api.ts"
      ],
      "@super-abp/ng.region-management/proxy": [
        "projects/region-management/proxy/src/public-api.ts"
      ],
      "@super-abp/ng.audit-logging": [
        "projects/audit-logging/src/public-api.ts"
      ],
      "@super-abp/ng.audit-logging/proxy": [
        "projects/audit-logging/proxy/src/public-api.ts"
      ],
      "@super-abp/ng.setting-management": [
        "projects/setting-management/src/public-api.ts"
      ],
      "@super-abp/ng.setting-management/config": [
        "projects/setting-management/config/src/public-api.ts"
      ],
      "@super-abp/ng.menu-management": [
        "projects/menu-management/src/public-api.ts"
      ],
      "@super-abp/ng.menu-management/proxy": [
        "projects/menu-management/proxy/src/public-api.ts"
      ],
      "@super-abp/ng.identity": ["projects/identity/src/public-api.ts"],
      "@super-abp/ng.identity/proxy": [
        "projects/identity/proxy/src/public-api.ts"
      ],
      "@super-abp/ng.tenant-management": [
        "projects/tenant-management/src/public-api.ts"
      ],
      "@super-abp/ng.tenant-management/proxy": [
        "projects/tenant-management/proxy/src/public-api.ts"
      ],
      "@shared": ["projects/dev-app/src/app/shared/index"],
      "@core": ["projects/dev-app/src/app/core/index"],
      "@proxy/*": ["projects/dev-app/src/app/proxy/*"],
      "@env/*": ["projects/dev-app/src/environments/*"]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
