{
  "name": "ng-packs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build_lib:identity": "ng build identity --configuration production",
    "npm_pack:identity": "cd dist/identity && npm pack",
    "package:identity": "npm run build_lib:identity && npm run npm_pack:identity",
    "build_lib:permission-management": "ng build permission-management --configuration production",
    "npm_pack:permission-management": "cd dist/permission-management && npm pack",
    "package:permission-management": "npm run build_lib:permission-management && npm run npm_pack:permission-management",
    "build_lib:menu-management": "ng build menu-management --configuration production",
    "npm_pack:menu-management": "cd dist/menu-management && npm pack",
    "package:menu-management": "npm run build_lib:menu-management && npm run npm_pack:menu-management"
  },
  "private": true,
  "dependencies": {
    "@abp/ng.core": "^6.0.0",
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@delon/abc": "^14.3.0",
    "@delon/acl": "^14.3.0",
    "@delon/auth": "^14.3.0",
    "@delon/cache": "^14.3.0",
    "@delon/form": "^14.3.0",
    "@delon/mock": "^14.3.0",
    "@delon/theme": "^14.3.0",
    "@delon/util": "^14.3.0",
    "@delon/chart": "^14.3.0",
    "ng-zorro-antd": "^14.2.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.2"
  }
}